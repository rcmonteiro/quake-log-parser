{
  "version": 2,
  "builds": [
    {
      "src": "src/http/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*",
          "tsconfig.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/http/server.ts"
    }
  ]
}